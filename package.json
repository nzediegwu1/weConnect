{
  "name": "weconnect",
  "version": "1.0.0",
  "description": "",
  "main": "server/app.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node server/app.js",
    "test": "npm run test:client && npm run coveralls",
    "build:server": "babel -d ./build ./server -s",
    "start": "node build/app.js",
    "test:server": "cross-env NODE_ENV=test npm run reset-migrate && cross-env NODE_ENV=test mocha server/test/**.spec.js --exit --require babel-register --reporter spec",
    "migrate": "sequelize db:migrate",
    "reset-migrate": "sequelize db:migrate:undo:all&&sequelize db:migrate",
    "coveralls": "nyc npm run test:server && nyc report --reporter=text-lcov | coveralls",
    "build": "npm run clean && npm run build:client && npm run build:server && cp server/index.html build && cp server/converted.json build",
    "clean": "rm -rf build && mkdir build",
    "server:coverage": "nyc report --reporter=text-lcov | coveralls",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seunzone/weConnect.git"
  },
  "author": "@seunzone",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/seunzone/weConnect/issues"
  },
  "homepage": "https://github.com/seunzone/weConnect#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "dotenv": "^5.0.1",
    "eslint": "^4.18.1",
    "express": "^4.16.2",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.5",
    "mocha": "^5.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.4.1",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.19.0",
    "sequelize-cli": "^3.0.0",
    "swagger-ui-express": "^2.0.15",
    "validator": "^9.4.1",
    "volleyball": "^1.4.1"
  },
  "devDependencies": {
    "nodemon": "^1.17.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0"
  }
}
