{
  "name": "weconnect",
  "version": "1.0.0",
  "description": "",
  "main": "server/app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production sequelize db:migrate --env production && babel-node server/dist/app.js",
    "start:dev": "nodemon server/app.js --exec babel-node",
    "build": "babel server -d server/dist",
    "db-migrate": "sequelize db:migrate",
    "test-migrate": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "test": "set NODE_ENV=test && npm run test-migrate && nyc mocha server/test/**.spec.js --timeout 15000 --compilers js:babel-core/register --exit",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postinstall": "npm run build",
    "webpack": "webpack-dev-server -d --config ./webpack.config.dev.js --hot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seunzone/weConnect.git"
  },
